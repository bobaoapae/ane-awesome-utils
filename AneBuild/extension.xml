<extension xmlns="http://ns.adobe.com/air/extension/51.1">
    <id>br.com.redesurftank.aneawesomeutils</id>
    <versionNumber>1</versionNumber>
    <platforms>
        <platform name="Android">
            <applicationDeployment>
                <nativeLibrary>app-debug.aar</nativeLibrary>
                <initializer>br.com.redesurftank.aneawesomeutils.AneAwesomeUtilsExtension</initializer>
            </applicationDeployment>
        </platform>
        <platform name="Windows-x86">
            <applicationDeployment>
                <nativeLibrary>AneAwesomeUtilsWindows.dll</nativeLibrary>
                <initializer>InitExtension</initializer>
                <finalizer>DestroyExtension</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Windows-x86-64">
            <applicationDeployment>
                <nativeLibrary>AneAwesomeUtilsWindows.dll</nativeLibrary>
                <initializer>InitExtension</initializer>
                <finalizer>DestroyExtension</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="MacOS-x86-64">
            <applicationDeployment>
                <nativeLibrary>AneAwesomeUtils.framework</nativeLibrary>
                <initializer>InitExtension</initializer>
                <finalizer>DestroyExtension</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libAneAwesomeUtils-IOS.a</nativeLibrary>
                <initializer>InitExtension</initializer>
                <finalizer>DestroyExtension</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment />
        </platform>
    </platforms>
</extension>
  